<% title t('.contact_us') %>

<!-- PAGE HEADING-->
<section class="section breadcrumb p-t-65 p-b-35">
    <div class="container">
        <div class="page-heading">
            <h2 class="title-2"><%= t('.contact_us') %></h2>
            <h4 class="title-sub title-sub--c8 m-b-15">
                <%= link_to t('home'), erp_saoee.root_path %> <i class="ti-angle-double-right"></i>
                <%= t('contact_us') %>
            </h4>
        </div>
    </div>
</section>
<!-- END PAGE HEADING-->

<!-- CONTACT-->
<section class="section p-b-80">
    <div class="container">
        <div class="row m-t-60">
            <div class="col-lg-5">
                <%= render "erp/saoee/frontend/modules/contact_us/contact_info" %>
            </div>
            <div class="col-lg-7">
                <%= render "erp/saoee/frontend/modules/contact_us/contact_form" %>
            </div>
        </div>
        <%= render "erp/saoee/frontend/modules/contact_us/google_map" %>
    </div>
</section>
<!-- END CONTACT-->